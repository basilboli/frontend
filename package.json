{
  "name": "bearded-web-frontend",
  "version": "0.0.5",
  "description": "Bearded web frontend application",
  "main": "app/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bearded-web/frontend.git"
  },
  "scripts": {
    "install-webdriver": "./node_modules/.bin/webdriver-manager update --standalone",
    "lint": "node_modules/.bin/eslint app/ --quiet",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build/ --inline",
    "deploy": "rm -rf dist && mkdir dist && cp favicons/* dist/ && NODE_ENV=production webpack -p",
    "test-server": "./node_modules/.bin/webdriver-manager start",
    "test-unit": "./node_modules/.bin/mocha --require pretest.js -R spec \"app/**/*.t.js\"",
    "coverage": "(cd app && ../node_modules/babel-istanbul/lib/cli.js --include-all-sources --babel-stage=0 cover ../node_modules/mocha/bin/_mocha -- -t 5000  -R spec \"./**/*.t.js\")",
    "test-e2e": "./node_modules/.bin/protractor protractor.js",
    "test": "npm run test-unit",
    "pretest": "npm run lint",
    "travis": "npm run lint && (cd app && ../node_modules/babel-istanbul/lib/cli.js --include-all-sources cover ../node_modules/mocha/bin/_mocha --report lcovonly -- -t 5000 -R spec \"./**/*.t.js\"  && cat ./coverage/coverage.json | ../node_modules/codecov.io/bin/codecov.io.js)"
  },
  "author": "Dmitry Manannikov <slonoed@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autobind-decorator": "^1.1.0",
    "babel": "5.2.6",
    "babel-core": "5.2.6",
    "babel-eslint": "*",
    "babel-istanbul": "0.1.8",
    "babel-loader": "^5.0.0",
    "blanket": "^1.1.6",
    "bootstrap": "^3.3.4",
    "chai": "^2.2.0",
    "classnames": "1.2.2",
    "codecov.io": "^0.1.0",
    "es6-promise": "2.1.1",
    "eslint": "*",
    "eslint-plugin-react": "*",
    "eventemitter3": "1.0.1",
    "flux": "2.0.3",
    "fluxxor": "^1.5.3",
    "font-awesome": "4.3.0",
    "font-awesome-webpack": "0.0.3",
    "immutable": "^3.7.1",
    "jed": "^1.1.0",
    "jsdom": "*",
    "less": "^2.5.0",
    "lodash": "^3.6.0",
    "markdown": "^0.5.0",
    "mocha": "^2.2.4",
    "mocha-jsdom": "^0.3.0",
    "mockery": "^1.4.0",
    "moment": "^2.10.0",
    "normalizr": "^0.1.3",
    "react": "0.13.2",
    "react-bootstrap": "0.21.0",
    "react-domify": "^0.2.1",
    "react-immutable-proptypes": "^0.1.3",
    "react-immutable-render-mixin": "^0.8.1",
    "react-router": "^0.13.2",
    "react-style": "^0.5.5",
    "react-tagsinput": "^1.3.2",
    "reqwest": "^1.1.5",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "swagger-client-generator": "^0.2.13",
    "tcomb": " 1.0.2",
    "tcomb-form": "0.4.11",
    "tcomb-json-schema": "^0.1.2",
    "tcomb-validation": "^1.0.3"
  },
  "devDependencies": {
    "chai-as-promised": "*",
    "css-loader": "^0.9.1",
    "extract-text-webpack-plugin": "0.7.0",
    "fetch-swagger-schema": "^0.1.2",
    "html-webpack-plugin": "1.3.0",
    "json-loader": "^0.5.1",
    "less-loader": "^2.2.0",
    "protractor": "^2.0.0",
    "react-hot-loader": "^1.2.4",
    "split-webpack-plugin": "https://github.com/slonoed/split-webpack-plugin.git",
    "style-loader": "0.12.1",
    "url-loader": "^0.5.5",
    "webpack": "1.8.10",
    "webpack-dev-server": "*"
  }
}
