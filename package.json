{
  "name": "bearded-web-frontend",
  "version": "0.0.3",
  "description": "Bearded web frontend application",
  "main": "app/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bearded-web/frontend.git"
  },
  "scripts": {
    "install-webdriver": "./node_modules/.bin/webdriver-manager update --standalone",
    "lint": "node_modules/.bin/eslint app/ --quiet",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build/ --inline",
    "deploy": "rm -rf dist && mkdir dist && cp favicons/* dist/ && NODE_ENV=production webpack -p",
    "test-server": "./node_modules/.bin/webdriver-manager start",
    "test-unit": "./node_modules/.bin/mocha --compilers js:babel/register -R nyan \"app/**/*.t.js\"",
    "coverage": "(cd app && ../node_modules/babel-istanbul/lib/cli.js  --include-all-sources cover ../node_modules/mocha/bin/_mocha -- -t 5000  -R spec \"./**/*.t.js\")",
    "test-e2e": "./node_modules/.bin/protractor protractor.js",
    "test": "npm run test-unit",
    "pretest": "npm run lint",
    "travis": "npm run lint && (cd app && ../node_modules/babel-istanbul/lib/cli.js --include-all-sources cover ../node_modules/mocha/bin/_mocha --report lcovonly -- -t 5000 -R spec \"app/**/*.t.js\"  && cat ./coverage/lcov.info | ../node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Dmitry Manannikov <slonoed@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^4.7.16",
    "babel-core": "^4.4.5",
    "babel-eslint": "*",
    "babel-istanbul": "^0.1.0",
    "babel-loader": "^5.0.0",
    "blanket": "^1.1.6",
    "bootstrap": "^3.3.4",
    "chai": "^2.2.0",
    "classnames": "^1.2.0",
    "css-loader": "^0.9.1",
    "es6-promise": "^2.0.1",
    "eslint": "*",
    "eventemitter3": "^0.1.6",
    "extract-text-webpack-plugin": "^0.5.0",
    "fetch-swagger-schema": "^0.1.2",
    "flux": "^2.0.1",
    "fluxxor": "^1.5.3",
    "fluxxor-test-utils": "^0.1.1",
    "font-awesome": "4.3.0",
    "font-awesome-webpack": "0.0.3",
    "html-webpack-plugin": "^1.1.0",
    "immutable": "^3.7.1",
    "jed": "^1.1.0",
    "jsdom": "^4.2.0",
    "json-loader": "^0.5.1",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "lodash": "^3.6.0",
    "markdown": "^0.5.0",
    "mocha": "^2.2.4",
    "mocha-jsdom": "^0.3.0",
    "mockery": "^1.4.0",
    "moment": "^2.10.0",
    "protractor": "^2.0.0",
    "react": "^0.13.1",
    "react-bootstrap": "^0.20.2",
    "react-domify": "^0.2.1",
    "react-hot-loader": "^1.2.4",
    "react-immutable-render-mixin": "^0.8.1",
    "react-markdown-el": "^1.0.1",
    "react-router": "^0.13.2",
    "react-tools": "^0.13.1",
    "reqwest": "^1.1.5",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "split-webpack-plugin": "git://github.com/SLonoed/split-webpack-plugin",
    "style-loader": "^0.9.0",
    "swagger-client-generator": "^0.2.13",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.2",
    "webpack-dev-server": "^1.8.0"
  },
  "dependencies": {
    "coveralls": "^2.11.2",
    "eslint-plugin-react": "^2.1.0",
    "tcomb": "^1.0.1",
    "tcomb-form": "^0.4.8",
    "tcomb-json-schema": "^0.1.2",
    "tcomb-validation": "^1.0.3"
  }
}
